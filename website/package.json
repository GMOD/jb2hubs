{
  "name": "website2",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf dist",
    "generate-taxonomy": "cd taxonomyBuilder && ./generate_taxonomy.sh",
    "prebuild": "yarn clean && yarn generate-taxonomy",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "sync": "aws s3 sync dist s3://genomes.jbrowse.org/ --size-only",
    "deploy": "yarn sync",
    "postdeploy": "AWS_PAGER=\"\" aws cloudfront  create-invalidation --distribution-id E12EBG02P68TDO --paths \"/*\"",
    "predeploy": "yarn build"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@floating-ui/react": "^0.27.16",
    "@fontsource/roboto": "^5.2.8",
    "@leeoniya/ufuzzy": "^1.0.18",
    "@tanstack/react-table": "^8.21.3",
    "@types/node": "^24.7.1",
    "@types/react": "^19.2.2",
    "astro": "5.14.1",
    "astro-seo": "^0.8.4",
    "lucide-react": "^0.545.0",
    "node-sql-parser": "^5.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-virtualized-sticky-tree": "^3.0.0-beta18"
  }
}
