---
import { hubCategories } from 'hubtools'
import a from './ui/a.astro'
---

<table>
  <thead>
    <tr>
      <th>hub&nbsp;gateway</th>
      <th>description</th>
    </tr>
  </thead>
  <tbody>
    {
      hubCategories
        .filter(f => f.tag !== 'main')
        .map(elt => (
          <tr key={elt.id}>
            <td>
              <a href={`/hubs/${elt.id}`}>{elt.id}</a>
            </td>
            <td>{elt.description}</td>
          </tr>
        ))
    }
  </tbody>
</table>
